@model SanalDenemem.MvcWebUI.Models.ExamResultSub

@{
    ViewBag.Title = Model.member.Name;
    Layout = "~/Views/Shared/_denemeLayout.cshtml";
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <link rel="stylesheet" href="~/Content/profil/css/animate.css">
    <title>Sanal Denemem </title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Online exam, çevrimiçi sınav , online sınav, sınavlar, online kurs" />
    <meta name="keywords" content="Online exam, çevrimiçi sınav , online sınav, sınavlar, online kurs" />
    <link rel="apple-touch-icon" href="images/ikon.ico" />
    <link rel="icon" type="image/ico" href="images/ikon.ico" />

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/Content/profil/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Content/profil/assets/vendor/icofont/icofont.min.css" rel="stylesheet">
    <link href="~/Content/profil/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/Content/profil/assets/vendor/venobox/venobox.css" rel="stylesheet">
    <link href="~/Content/profil/assets/vendor/owl.carousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Content/profil/assets/vendor/aos/aos.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/Content/profil/assets/css/style.css" rel="stylesheet">


</head>
<style>
.skill{
    font-size:20px

}
.card-header{

    font-size:20px;
    text-align:center;
    font-weight:bold;
}
.card-title{
    font-size:14px;
    color:white;
    

}
</style>
<body>

    <!-- ======= Mobile nav toggle button ======= -->
    <button type="button" class="mobile-nav-toggle d-xl-none"><i class="icofont-navigation-menu"></i></button>

    <!-- ======= Header ======= -->
    <header id="header">
        <div class="d-flex flex-column">

            <div class="profile">
                <img src="assets/img/profile-img.jpg" alt="" class="img-fluid rounded-circle">
                <h1 class="text-light"><a href="index.html">Yunus Şen</a></h1>

            </div>

            <nav class="nav-menu">
                <ul>
                    <li class="active"><a href="#facts"><i class="bx bx-home"></i> <span>Menu</span></a></li>
                    <li><a href="#skills"><i class="bx bx-file-blank"></i> <span>İstatistikler</span></a></li>
                    <li><a href="#portfolio"><i class="bx bx-book-content"></i> <span>Sınav Sonuçları</span></a></li>
                    <li><a href="@Url.Action("Index","Home")"><i class="icon-arrow-back"></i> <span>Çıkış Yap</span></a></li>

                </ul>
            </nav><!-- .nav-menu -->
            <button type="button" class="mobile-nav-toggle d-xl-none"><i class="icofont-navigation-menu"></i></button>

        </div>
    </header><!-- End Header -->



    <main id="main">



        <!-- ======= Facts Section ======= -->
        <section id="facts" class="facts">
            <div class="container">

                <div class="section-title">
                    <h2>Giriş Skorları</h2>

                </div>

                <div class="row no-gutters">

                    <div class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch" data-aos="fade-up">

                        <div class="count-box">
                            <i class="icofont-paper"></i>
                            <span data-toggle="counter-up">57</span>
                            <p><strong> Sınava</strong> katılım sağlandı.</p>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch" data-aos="fade-up" data-aos-delay="100">
                        <div class="count-box">
                            <i class="icofont-pen-alt-1"></i>
                            <span data-toggle="counter-up">9120</span>
                            <p><strong> Soru </strong> çözümü yapıldı.</p>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch" data-aos="fade-up" data-aos-delay="200">
                        <div class="count-box">
                            <i class="icofont-student-alt"></i>
                            <span data-toggle="counter-up">1,758</span>
                            <p><strong>Kişi</strong> ile aynı sınavda derece tespiti yapıldı.</p>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-6 d-md-flex align-items-md-stretch" data-aos="fade-up" data-aos-delay="300">
                        <div class="count-box">
                            <i class="icofont-clock-time"></i>
                            <span data-toggle="counter-up">6840</span>
                            <p><strong>Dakika </strong> sitede vakit geçirildi.</p>
                        </div>
                    </div>

                </div>

            </div>
        </section><!-- End Facts Section -->
        <!-- ======= Skills Section ======= -->
        <section id="skills" class="skills section-bg">
            <div class="container">

                <div class="section-title">
                    <h2>Ders Başarı Oranı</h2>

                </div>

                <div class="row skills-content">

                    <div class="col-lg-6" data-aos="fade-up">

                        <div class="progress">
                            <span class="skill">Türkçe <i class="val">73%</i></span>
                            <div class="progress-bar-wrap">
                                <div class="progress-bar" role="progressbar" aria-valuenow="73" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </div>

                        <div class="progress">
                            <span class="skill">Matematik <i class="val">86%</i></span>
                            <div class="progress-bar-wrap">
                                <div class="progress-bar" role="progressbar" aria-valuenow="86" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </div>

                        <div class="progress">
                            <span class="skill">Fizik <i class="val">83%</i></span>
                            <div class="progress-bar-wrap">
                                <div class="progress-bar" role="progressbar" aria-valuenow="83" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </div>

                    </div>

                    <div class="col-lg-6" data-aos="fade-up" data-aos-delay="100">

                        <div class="progress">
                            <span class="skill">Kimya <i class="val">57%</i></span>
                            <div class="progress-bar-wrap">
                                <div class="progress-bar" role="progressbar" aria-valuenow="57" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </div>

                        <div class="progress">
                            <span class="skill">Biyoloji<i class="val">43%</i></span>
                            <div class="progress-bar-wrap">
                                <div class="progress-bar" role="progressbar" aria-valuenow="43" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </div>

                        <div class="progress">
                            <span class="skill">Tarih <i class="val">55%</i></span>
                            <div class="progress-bar-wrap">
                                <div class="progress-bar" role="progressbar" aria-valuenow="55" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                        </div>

                    </div>

                </div>

            </div>
        </section><!-- End Skills Section -->
        <!-- ======= Portfolio Section ======= -->
        <section id="portfolio" class="portfolio section-bg">
            <div class="container">

                <div class="section-title">
                    <h2>Sınav Sonuçları</h2>

                </div>

                <div class="row" data-aos="fade-up">
                    <div class="col-lg-12 d-flex justify-content-center">
                        <ul id="portfolio-flters">
                            <li data-filter="*" class="filter-active">Tüm Sınavlar</li>
                            <li data-filter=".filter-yks">YKS</li>
                            <li data-filter=".filter-tyt">TYT</li>
                            <li data-filter=".filter-ayt">AYT</li>
                            <li data-filter=".filter-dhpt">DHPT</li>
                        </ul>
                    </div>
                </div>

                <div class="row portfolio-container" data-aos="fade-up" data-aos-delay="100">

                    <div class=" col-md-2 portfolio-item filter-yks">
                        <div class="portfolio-wrap">


                            <div class="card text-white bg-secondary mb-3" style="max-width: 20rem;">
                                <div class="card-header">YKS</div>
                                <div class="card-body">
                                    <h5 class="card-title">Doğru Sayısı:115</h5>
                                    <h5 class="card-title">Yanlış Sayısı: 21</h5>
                                    <h5 class="card-title">Net Sayısı: 100</h5>
                                </div>

                            </div>

                        </div>
                    </div>


                    <div class=" col-md-2 portfolio-item filter-tyt">
                        <div class="portfolio-wrap">


                            <div class="card text-white bg-success mb-3" style="max-width: 20rem;">
                                <div class="card-header">TYT</div>
                                <div class="card-body">
                                    <h5 class="card-title">Doğru Sayısı:115</h5>
                                    <h5 class="card-title">Yanlış Sayısı: 21</h5>
                                    <h5 class="card-title">Net Sayısı: 100</h5>
                                </div>

                            </div>

                        </div>
                    </div>

                    <div class=" col-md-2 portfolio-item filter-ayt">
                        <div class="portfolio-wrap">


                            <div class="card text-white bg-warning mb-3" style="max-width: 20rem;">
                                <div class="card-header">AYT</div>
                                <div class="card-body">
                                    <h5 class="card-title">Doğru Sayısı:115</h5>
                                    <h5 class="card-title">Yanlış Sayısı: 21</h5>
                                    <h5 class="card-title">Net Sayısı: 100</h5>
                                </div>

                            </div>

                        </div>
                    </div>

                    <div class=" col-md-2 portfolio-item filter-dhpt">
                        <div class="portfolio-wrap">


                            <div class="card text-white bg-info mb-3" style="max-width: 20rem;">
                                <div class="card-header">DHPT</div>
                                <div class="card-body">
                                    <h5 class="card-title">Doğru Sayısı:115</h5>
                                    <h5 class="card-title">Yanlış Sayısı: 21</h5>
                                    <h5 class="card-title">Net Sayısı: 100</h5>
                                </div>

                            </div>

                        </div>
                    </div>

                   

                </div>

            </div>
        </section><!-- End Portfolio Section -->



    </main><!-- End #main -->



    <a href="#" class="back-to-top"><i class="icofont-simple-up"></i></a>

    <!-- Vendor JS Files -->
    <script src="~/Content/profil/assets/vendor/jquery/jquery.min.js"></script>
    <script src="~/Content/profil/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Content/profil/assets/vendor/jquery.easing/jquery.easing.min.js"></script>
    <script src="~/Content/profil/assets/vendor/php-email-form/validate.js"></script>
    <script src="~/Content/profil/assets/vendor/waypoints/jquery.waypoints.min.js"></script>
    <script src="~/Content/profil/assets/vendor/counterup/counterup.min.js"></script>
    <script src="~/Content/profil/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/Content/profil/assets/vendor/venobox/venobox.min.js"></script>
    <script src="~/Content/profil/assets/vendor/owl.carousel/owl.carousel.min.js"></script>
    <script src="~/Content/profil/assets/vendor/typed.js/typed.min.js"></script>
    <script src="~/Content/profil/assets/vendor/aos/aos.js"></script>

    <!-- Template Main JS File -->
    <script src="~/Content/profil/assets/js/main.js"></script>

</body>

</html>
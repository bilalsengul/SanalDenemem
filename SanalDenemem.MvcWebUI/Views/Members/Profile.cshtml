        <section id="portfolio" class="portfolio section-bg">
            <div class="container">

                <div class="section-title">
                    <h2>Sınav Sonuçları</h2>

                </div>

                <div class="row" data-aos="fade-up">
                    <div class="col-lg-12 d-flex justify-content-center">
                        <ul id="portfolio-flters">
                            <li data-filter="*" class="filter-active">Tüm Sınavlar</li>
                            @foreach (var item in Model.ExamTypes)
                            {
                                <li data-filter=".filter-@item.Id">@item.ExamTypeName</li>
                            }
                        </ul>
                    </div>
                </div>

                <div class="row portfolio-container" data-aos="fade-up" data-aos-delay="100">

                    @foreach (var item in Model.ExamTypes)
                    {
                        foreach (var item2 in item.Exams)
                        {
                            if (Model.exams.Contains(item2))
                            {
                                <a target="_blank" href="@Url.Action("ExamToMemberExam", "Members",new {id=item2.Id })">
                                    <div class="col-md-2 portfolio-item filter-@item.Id">
                                        <div class="portfolio-wrap">
                                            <div class="card text-white bg-success mb-3s" style="max-width: 20rem;">
                                                <div class="card-header">@item2.Title</div>
                                            </div>
                                        </div>
                                    </div>
                                </a>
                            }
                        }
                    }
                </div>
            </div>
            <br />
            <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
        </section>



